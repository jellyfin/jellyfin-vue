{
  "name": "@jellyfin-vue/ui-toolkit",
  "type": "module",
  "description": "Base components and UI toolkit for Jellyfin Vue",
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    "./composables/*.ts": "./src/composables/*.ts",
    "./components": "./src/components/index.ts",
    "./store": "./src/store.ts",
    "./resolver": "./src/resolver.ts"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "eslint-config-inspector",
    "check:types": "vue-tsc",
    "storybook": "storybook dev -p 6006 --config-dir storybook --no-open"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.6",
    "@jellyfin-vue/configs": "*",
    "@storybook/addon-a11y": "8.6.11",
    "@storybook/addon-essentials": "8.6.11",
    "@storybook/addon-interactions": "8.6.11",
    "@storybook/blocks": "8.6.11",
    "@storybook/vue3": "8.6.11",
    "@storybook/vue3-vite": "8.6.11",
    "unplugin-vue-components": "28.4.1",
    "vite": "6.2.6"
  },
  "dependencies": {
    "@skirtle/vue-vnode-utils": "0.2.0",
    "@vueuse/core": "13.0.0",
    "color": "5.0.0",
    "comlink": "4.4.2",
    "vue": "3.5.13",
    "vue-i18n": "9.14.3",
    "vuetify": "3.8.0"
  }
}
