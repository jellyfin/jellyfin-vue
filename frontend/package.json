  "scripts": {
    "analyze": "vite build --mode analyze",
    "build": "vite build",
    "check": "npm run lint && npm run typecheck",
    "start": "vite",
    "serve": "vite preview",
    "lint": "prettier . --check --ignore-path ../.gitignore && eslint . --ext .ts,.js,.json,.vue --ignore-path ../.gitignore",
    "lint-fix": "prettier . --write --ignore-path ../.gitignore && eslint . --fix --ext .ts,.js,.json,.vue --ignore-path ../.gitignore",
    "prod": "npm run build && npm run serve",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "vue-i18n-extract": "npx vue-i18n-extract",
    "vue-i18n-extract-remove": "npx vue-i18n-extract --remove"
  },
